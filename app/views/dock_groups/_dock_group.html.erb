<tr id="dock_group_<%= dock_group.id %>">
  <th scope="row">
    <%= link_to 'Edit', edit_dock_group_path(dock_group), :remote => true, :'data-toggle' =>  "modal", :'data-target' => '#update-modal', :class => 'btn btn-primary btn-sm m-1'  %>
  </th>
  <td>
    <%= link_to dock_group.description, dock_group_path(dock_group),  {:remote => true, 'data-toggle' =>  "modal", 'data-target' => '#show-modal' }  %>
  </td>
  <td>
    <%= human_boolean(dock_group.enabled) %>
  </td>
</tr>
