<% if @dock_group.nil? %>
  alert_custom("warning","Dock group no longer exists");
<% elsif @dock_group.valid? %>
  $("#modal-wrapper").html("<%= j render 'shared/generic_modal', { :form_type => 'update', :title => 'Edit Dock Group', :form_path => 'dock_groups/new_edit_form' } %>");
  $("#update-modal").modal('show');
<% end %>
