<% if !@current_dock_group.nil? %>
  selected_dock_group = $("#dock_group_select").val();
  $("#modal-wrapper").html("<%= j render 'shared/generic_modal', { :form_type => 'create', :title => 'New Dock Request', :form_path => 'dock_requests/new_edit_form' } %>");
  $("#create-modal").modal('show');
  if (typeof selected_dock_group !== "undefined") {
    $("#dock_request_dock_group_id").val(selected_dock_group);
  } else {
    $("#dock_request_dock_group_id").val(<%= @current_dock_group.id %>);
  }
<% else %>
  alert_custom("warning","No dock group selected.")
<% end %>
