<% if @dock_request.nil? %>
  alert_custom("danger","<%= DockRequest.no_longer_exists_alert_message %>");
<% elsif @dock_request.valid? %>
  $("#modal-wrapper").html("<%= j render 'shared/generic_modal', { :form_type => 'show', :title => 'Dock Request' } %>");
  $("#show-modal").modal('show');
<% end %>
