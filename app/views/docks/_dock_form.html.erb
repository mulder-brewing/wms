<%= simple_form_for modal.form, remote:true, :html => { :id => modal.form.html_id } do |f| %>
    <div id="feedback">
      <%= errors_for(f.object) %>
    </div>
    <div class="mb-3">
      <%= f.input :number, as: :string, required: true %>
      <%= f.input :dock_group_id, label: t_nf("dock_groups.dock_group"),
        :collection => modal.form.dock_groups, :label_method => :description,
        :value_method => :id, required: true, prompt: true %>
      <% if modal.role?(:edit_update) %>
        <div class="mb-2">
          <%= f.input :enabled, sf_switch %>
        </div>
      <% end %>
    </div>
<% end %>
