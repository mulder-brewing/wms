<% if @dock.nil? %>
  $('#update-modal').modal('hide');
  alert_custom("warning","Dock no longer exists");
<% elsif @dock.save_success %>
  $('#update-modal').modal('hide');
  $("#dock_<%=@dock.id%>").replaceWith('<%= j render @dock, dock: @dock %>');
  alert_custom("success", "Dock updated successfully!")
<% else %>
  $("#update-form").replaceWith("<%=  j render 'docks/new_edit_form', form_type: 'update' %>");
<% end %>
